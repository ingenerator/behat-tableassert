{
  "name": "ingenerator/behat-tableassert",
  "description": "Assertions for all sorts of tabular data in behat",
  "homepage": "https://github.com/ingenerator/behat-tableassert",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Andrew Coulton",
      "email": "andrew@ingenerator.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "behat/gherkin": ">=2.0.0 <5.0.0",
    "php": "^7.4 || ~8.0.0 || ~8.1.0",
    "ext-dom": "*",
    "ext-SimpleXML": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "behat/mink": "^1.7"
  },
  "suggest": {
    "behat/mink": "For parsing tables from a browser response"
  },
  "support": {
    "source": "https://github.com/ingenerator/behat-tableassert/",
    "issues": "https://github.com/ingenerator/behat-tableassert/issues"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\BehatTableAssert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\mock\\Ingenerator\\BehatTableAssert\\": "test/mock/",
      "test\\Ingenerator\\BehatTableAssert\\": "test/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
